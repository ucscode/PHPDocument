    "keywords": ["php", "html", "dom", "element", "library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Uchenna Ajah",
            "email": "uche23mail@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Ucscode\\PHPDocument\\": "src/",
            "Ucscode\\PHPDocument\\Test\\": "tests/"
        }
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
